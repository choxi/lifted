!!!
%html{"ng-app" => "Lifted"}
  %head
    %title
      = content_for?(:title) ? yield(:title) : "Lifted"
    %meta{"http-equiv"=>"Content-Type", content:"text/html; charset=utf-8"}/
    = stylesheet_link_tag :application
    = javascript_include_tag :application
    = csrf_meta_tag
    = yield(:head)

  %body
    .container
      = yield
